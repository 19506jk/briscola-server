{
  "name": "briscola-server",
  "version": "1.0.1",
  "private": true,
  "config": {
    "serverPort": 8080,
    "serverSocketPort": 7777
  },
  "scripts": {
    "start": "./node_modules/.bin/nodemon ./src/app.js --exec babel-node",
    "build": "./node_modules/.bin/babel src -d dist",
    "eslint": "./node_modules/.bin/eslint .",
    "mocha-test": "mocha --require babel-core/register --exit",
    "test": "nyc mocha --require babel-core/register --exit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "mock": "./node_modules/.bin/babel-node ./src/mockGame.js"
  },
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "nyc": "^11.4.1",
    "socket.io-client": "^2.0.4"
  }
}
